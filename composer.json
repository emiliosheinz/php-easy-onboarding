{
  "name": "emiliosheinz/php-easy-onboarding",
  "description": "Experimental code for PHP learning, building a property onboarding API.",
  "type": "project",
  "require": {
    "symfony/validator": "^7.0",
    "symfony/intl": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.1",
    "phpunit/php-code-coverage": "^11.0",
    "squizlabs/php_codesniffer": "^3.9",
    "slevomat/coding-standard": "^8.15"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-0": {
      "": "src/"
    }
  },
  "authors": [
    {
      "name": "Emilio Schaedler Heinzmann",
      "email": "emiliosheinz@gmail.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
